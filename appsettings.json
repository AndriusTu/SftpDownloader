{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Quartz": "Warning",
        "Microsoft": "Error"
      }
    }
  },
  "ConnectionStrings": {
    "Database": "Server=127.0.0.1;Port=5432;Database=FileDb;Integrated Security=true;"
  },
  "SftpConfiguration": {
    "Host": "localhost",
    "Port": 22,
    "Username": "your-sftp-username",
    "Password": "your-sftp-password",
    "RemotePath": "C:/SFTPRoot/files",
    "LocalPath": "C:/files/"
  }
}
